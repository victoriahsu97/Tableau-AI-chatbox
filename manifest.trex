<?xml version="1.0" encoding="utf-8"?>
<Extension xmlns="http://tableau.com/api" version="1.0.0">

  <DashboardExtension>
  
    <Name>ChatGPT Chatbox</Name>
    <Description>ChatGPT extension for Tableau</Description>
    <Version>1.0.0</Version>

    <!-- GitHub Pages UI -->
    <Source>https://victoriahsu97.github.io/Tableau-AI-chatbox/index.html</Source>

    <!-- Allow connection to Render backend -->
    <Permissions>
      <NetworkAccess>
        <Domains>
          <Domain>https://tableau-ai-chatbox.onrender.com</Domain>
        </Domains>
      </NetworkAccess>
    </Permissions>

  </DashboardExtension>

</Extension>
